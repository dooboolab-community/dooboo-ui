{
  "homepage": "https://dooboolab.github.io/dooboo-ui/",
  "private": true,
  "scripts": {
    "pre": "lerna clean --yes && lerna bootstrap && lerna run build",
    "predeploy": "yarn build",
    "postinstall": "dooboolab-welcome postinstall && rm -rf storybook && cp -R .storybook storybook",
    "publish": "yarn build && cd lib && npm publish && cd ..",
    "publish:packages": "yarn pre && yarn build:packages && lerna exec -- npm publish",
    "lint": "eslint main packages stories docs --ext .ts,.tsx,.js,.jsx",
    "tsc": "tsc",
    "start": "expo start",
    "jest": "jest --runInBand",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "start-storybook -p 6006",
    "eject": "expo eject",
    "test": "jest",
    "test:all": "lerna run build && yarn lint && yarn tsc && jest",
    "test:post": "jest",
    "prebuild": "cp ./main/react-native.config.js ./lib/react-native.config.js && rm -rf ./lib/tsconfig* && cp README.md ./lib/README.md ",
    "build": "yarn prebuild && tsc --project tsconfigBuild.json && yarn cp:icon",
    "cp:icon": "mkdir -p `dirname ./lib/Icon/selection.json` && cp ./main/Icon/selection.json ./lib/Icon/selection.json && cp ./main/Icon/doobooui.ttf ./lib/Icon/doobooui.ttf",
    "postbuild": "cp -R main/__assets__ lib",
    "build:storybook": "expo build:web && sh web-build-postinstall.sh",
    "deploy:storybook": "gh-pages -d web-build",
    "build:packages": "lerna exec -- yarn build",
    "install:packages": "lerna exec -- yarn",
    "upgrade:packages": "lerna exec -- ncu -u && yarn install:packages"
  },
  "dependencies": {
    "@emotion/native": "^11.0.0",
    "@emotion/react": "^11.4.1",
    "@expo/match-media": "^0.1.0",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "dooboolab-welcome": "^1.3.2",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-vector-icons": "^8.1.0",
    "react-native-web-hooks": "^3.0.1",
    "react-responsive": "^8.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@dooboo/eslint-config": "^0.8.3",
    "@emotion/babel-plugin": "^11.3.0",
    "@expo/match-media": "^0.1.0",
    "@expo/webpack-config": "^0.15.0",
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@storybook/addon-actions": "^6.3.10",
    "@storybook/addon-controls": "^6.3.7",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addons": "^6.3.7",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.3.7",
    "@storybook/react-native": "5.3.25",
    "@storybook/react-native-server": "5.3.23",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^27.0.1",
    "@types/jest-plugin-context": "^2.9.4",
    "@types/react": "17.0.8",
    "@types/react-dom": "17.0.9",
    "@types/react-native": "0.64.13",
    "@types/react-responsive": "^8.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "babel-loader": "^8.2.2",
    "babel-preset-expo": "^8.4.1",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.32.0",
    "expo": "^42.0.3",
    "expo-constants": "~11.0.1",
    "expo-font": "~9.2.1",
    "expo-linear-gradient": "~9.2.0",
    "gh-pages": "^3.2.3",
    "givens": "^1.3.9",
    "intl": "^1.2.5",
    "jest": "^27.1.0",
    "jest-expo": "^42.1.0",
    "jest-plugin-context": "^2.9.0",
    "lerna": "^4.0.0",
    "react": "17.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "17.0.2",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-get-random-values": "^1.7.0",
    "react-native-svg": "^12.1.1",
    "react-native-web": "~0.17.1",
    "react-test-renderer": "^17.0.2",
    "react-useinterval": "^1.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "4.3.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
