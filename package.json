{
  "homepage": "https://dooboolab.github.io/dooboo-ui",
  "private": true,
  "scripts": {
    "pre": "lerna run clean && lerna clean --yes && lerna bootstrap && lerna run build",
    "predeploy": "yarn build",
    "postinstall": "dooboolab-welcome postinstall && rm -rf storybook && cp -R .storybook storybook",
    "publish": "yarn build && cd lib && npm publish && cd ..",
    "publish:packages": "yarn pre && yarn build:packages && lerna exec -- npm publish",
    "lint": "eslint main packages docs --ext .ts,.tsx,.js,.jsx",
    "lint:ci": "commitlint --from HEAD~0 --to HEAD --verbose",
    "tsc": "tsc",
    "start": "expo start",
    "jest": "jest --runInBand",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test:pre": "mv packages/theme/package.json packages/theme/packages.json",
    "test:post": "mv packages/theme/packages.json packages/theme/package.json",
    "test": "yarn test:pre; jest; yarn test:post;",
    "test:all": "lerna run build && yarn lint && yarn tsc && yarn test",
    "build": "tsc --project tsconfigBuild.json && yarn cp:icon",
    "cp:icon": "mkdir -p `dirname ./lib/components/Icon/selection.json` && cp ./main/components/Icon/selection.json ./lib/components/Icon/selection.json && cp ./main/components/Icon/doobooui.ttf ./lib/components/Icon/doobooui.ttf",
    "postbuild": "cp ./main/react-native.config.cjs ./lib/react-native.config.cjs && rm -rf ./lib/tsconfig* && cp README.md ./lib/README.md && cp -R main/__assets__ lib",
    "build:storybook": "expo export:web && sh web-build-postinstall.sh",
    "deploy:storybook": "gh-pages -d web-build",
    "build:packages": "lerna exec -- yarn build",
    "install:packages": "lerna exec -- yarn",
    "upgrade:packages": "lerna exec -- ncu -u && yarn install:packages",
    "prepare": "husky install"
  },
  "dependencies": {
    "@dooboo-ui/theme": "^0.10.3",
    "@emotion/native": "^11.10.0",
    "@emotion/react": "^11.10.5",
    "@expo/match-media": "^0.4.0",
    "@expo/webpack-config": "^0.17.3",
    "dooboolab-welcome": "^1.3.2",
    "expo": "^47.0.8",
    "expo-constants": "~14.0.2",
    "expo-font": "~11.0.1",
    "expo-linear-gradient": "~12.0.1",
    "expo-screen-orientation": "~5.0.1",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-modalbox": "^2.0.2",
    "react-native-svg": "13.4.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.10",
    "react-native-web-hooks": "^3.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.19.6",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@dooboo/eslint-config-react-native": "^1.0.4",
    "@jest/types": "^29.3.1",
    "@react-native-async-storage/async-storage": "~1.17.11",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-controls": "^6.5.13",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.3.1",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.26",
    "@storybook/addons": "^6.5.13",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.5.13",
    "@storybook/react-native": "5.3.27",
    "@storybook/react-native-server": "5.3.23",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/jest-native": "^5.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^29.2.3",
    "@types/jest-plugin-context": "^2.9.5",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@types/react-native": "^0.70.7",
    "@types/react-responsive": "^8.0.5",
    "@types/react-test-renderer": "^18.0.0",
    "babel-loader": "^8.2.5",
    "babel-preset-expo": "9.2.2",
    "babel-preset-react-app": "^10.0.1",
    "commitlint-plugin-function-rules": "^1.7.1",
    "eslint": "^8.28.0",
    "gh-pages": "^4.0.0",
    "givens": "^1.3.9",
    "husky": "^8.0.2",
    "intl": "^1.2.5",
    "jest": "^29.3.1",
    "jest-expo": "^47.0.1",
    "jest-plugin-context": "^2.9.0",
    "lerna": "^6.0.3",
    "react": "18.1.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "18.1.0",
    "react-test-renderer": "^18.1.0",
    "react-useinterval": "^1.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.3",
    "webpack-dev-server": "^3.11.3"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  }
}
