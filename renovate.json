{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "prHourlyLimit": 3,
  "prConcurrentLimit": 5,
  "packageRule": {
    "excludeDepNames": [
      "expo-font",
      "babel-loader",
      "react",
      "react-native",
      "react-native-reanimated",
      "@expo/webpack-config",
      "webpack-dev-server"
    ]
  }
}
